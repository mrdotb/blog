{
  "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "indent": ["error", 2],
      "semi": ["error", "never"],
      "quotes": ["error", "single"]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
    "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
}
